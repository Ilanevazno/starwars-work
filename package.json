    "@types/react-dom": "^16.9.17",
    "@types/react-redux": "^7.1.24",
    "antd": "^5.4.6",
    "axios": "^1.3.6",
    "axios-case-converter": "^1.1.0",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
